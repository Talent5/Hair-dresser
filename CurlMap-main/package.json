{
  "name": "curlmap-stylist-app",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:no-telemetry": "EXPO_NO_TELEMETRY=1 expo start",
    "start:clean": "expo start --clear",
    "dev": "expo start",
    "dev:no-telemetry": "EXPO_NO_TELEMETRY=1 expo start",
    "dev:clean": "expo start --clear",
    "prebuild": "expo prebuild --clean",
    "prebuild:clean": "expo prebuild --clean && npx react-native clean",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "expo lint",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "clean": "expo install --fix && npm cache clean --force",
    "reset": "npx react-native clean && npm run clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@maplibre/maplibre-react-native": "^10.2.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@reduxjs/toolkit": "^2.2.0",
    "axios": "^1.6.7",
    "expo": "~53.0.23",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "metro": "^0.82.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1305.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-redux": "^9.1.0",
    "socket.io-client": "^4.7.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
